{
  "name": "@wochit/embedded",
  "version": "0.0.0",
  "private": false,
  "description": "description",
  "files": [
    "dist"
  ],
  "main": "./dist/module.umd.js",
  "module": "./dist/module.es.js",
  "exports": {
    ".": {
      "import": "./dist/module.es.js",
      "require": "./dist/module.umd.js"
    }
  },
  "types": "./dist/module.d.ts",
  "scripts": {
    "dev:module": "tsc && vite build --config vite-module.config.js --watch",
    "dev:snippet": "tsc && vite build --config vite-snippet.config.js --watch",
    "test": "echo \"test\" && exit 0",
    "build:module": "tsc && vite build --config vite-module.config.js",
    "build:snippet": "tsc && vite build --config vite-snippet.config.js",
    "build:snippet:min": "tsc && vite build --config vite-snippet-min.config.js",
    "build:all": "npm run build:module && npm run build:snippet && npm run build:snippet:min"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wochit/wochit-embedded.git"
  },
  "keywords": [
    "wochit",
    "wochit-inside",
    "site-integration",
    "embedded-video-editor"
  ],
  "author": "Wochit LTD",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/wochit/wochit-embedded/issues"
  },
  "homepage": "https://github.com/wochit/wochit-embedded#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.6.0",
    "typescript": "^4.6.2",
    "vite": "^2.8.6"
  }
}
